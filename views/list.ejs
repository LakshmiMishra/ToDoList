
<%- include('header'); -%>
    <div class="box" id="heading">
      <h1><%=listTitle %></h1>
    </div>
    <div class="box">

        <% for(var i=0;i<newtodoItem.length ;i++){%>
          <div class="item">
            <input type="checkbox">
            <p>
              <%=newtodoItem[i]%>
            </p>
          </div>

          <%}%>
          <form class="item" action="/" method="post">

            <input type="text" name="todoItem" value="" placeholder="New Item" autocomplete="off">
            <button type="submit" name="listName" value="<%=listTitle%>">+</button>

          </form>
    </div>
<%- include('footer'); -%>
